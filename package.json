{
  "name": "clustery",
  "version": "0.0.2-11",
  "description": "A node cluster manager",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "release": "node fuse -p && np"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmseguin/clustery.git"
  },
  "author": "Charles-Mathieu Seguin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cmseguin/clustery/issues"
  },
  "homepage": "https://github.com/cmseguin/clustery#readme",
  "dependencies": {
    "bluebird": "3.5.1",
    "lodash": "4.17.4"
  },
  "devDependencies": {
    "fuse-box": "3.0.2",
    "jest": "22.0.6",
    "np": "2.18.3",
    "typescript": "2.6.2",
    "uglify-es": "3.3.6",
    "uglify-js": "3.3.6",
    "yargs": "10.1.1"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "collectCoverageFrom": [
      "**/src/**/*.{js}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/tests/**"
    ],
    "coverageDirectory": "coverage",
    "testMatch": [
      "<rootDir>/tests/**/*.spec.js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/coverage/",
      "/data/",
      ".*/_.*"
    ]
  }
}
